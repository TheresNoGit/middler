{
    "name": "theresnogit/middler",
    "type": "project",
    "description": "middler is a quick and dumb bit of middleware so that GitHub Webhooks can talk to Discord Webhooks.",
    "license": "MIT",
    "autoload": {
        "psr-4": {"middler\\": "includes/middler/"}
    },
    "require": {
        "php-parallel-lint/php-parallel-lint": "^1.3"
    },
    "scripts": {
        "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
        "lint:php:all": "@php ./vendor/bin/parallel-lint ."
    }
}
